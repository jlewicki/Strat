      <Compile Include="Collections\Vector.fs" />
      <Compile Include="Collections\IndexList.fsi" />
      <Compile Include="Collections\IndexList.fs" />
      <Compile Include="Lenses\Lens.fsi" />
      <Compile Include="Lenses\Lens.fs" />
      <Compile Include="Lenses\Lenses.fs" />
   </ItemGroup>

</Project>
